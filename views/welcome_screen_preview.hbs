<div class="gallery">
    {{#each images}}
        <section>
            <img class="current_image_with_nav" src="/uploads/{{this.imageName}}"/>
            <ul class="list-group row guests_list">
                <li class="list-group-item col-xs-12 company_name">{{this.company}}:</li>
                {{#each this.guestsNames}}
                    {{#guestList this}}
                    {{/guestList }}
                {{/each}}
            </ul>
        </section>
    {{/each}}
    {{#each videos}}
        <section>
            <video class="current_video_with_nav" autoplay muted loop preload>
                <source src="/uploads/{{this.videoName}}" type="video/mp4"></source>
            </video>
        </section>
    {{/each}}
</div>